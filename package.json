{
  "name": "jest-marmot",
  "version": "1.0.5",
  "description": "Jest Test Framework DSL",
  "main": "lib/jest-marmot.js",
  "files": [
    "lib/jest-marmot.js",
    "lib/jest-marmot.min.js"
  ],
  "repository": "git@github.com:axehomeyg/jest-marmot.git",
  "author": "Michael Cerna <cerna5150@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "@testing-library/react": "^8.0.1",
    "@testing-library/user-event": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-jest": "^22.7.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.1.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "yargs": "^13.2.1"
  },
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest test",
    "test:watch": "jest --watch"
  },
  "jest": {
    "automock": false,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 92,
        "functions": 81,
        "lines": 92,
        "statements": 87
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/mocks/"
    ]
  }
}
